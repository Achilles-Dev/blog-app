<h1>Add New Post</h1>
<div class='post-form'>
  <%= form_with model: @post do |form| %>
    <div>
      <%= form.label :title %>
      <%= form.text_field :title%>
    </div>
    <div>
      <%= form.label :text %>
      <%= form.text_area :text, size: "40x5"%>
    </div>
    <%= form.submit 'Add Post' %>
  <% end %>
</div>